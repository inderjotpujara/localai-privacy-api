# Environment Configuration for Testing
# Copy this file to .env.test for running tests

NODE_ENV=test
PORT=3001
JWT_SECRET=test-secret-key-for-testing-only
LOG_LEVEL=silent

# Test Database (use a separate test database)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/localllm_test

# Mock LocalAI for testing
LOCALAI_URL=http://localhost:8080
LOCALAI_MODEL=test-model

# Test-specific settings
EMBEDDING_MODEL=test-embedding-model
VECTOR_DIMENSIONS=384
